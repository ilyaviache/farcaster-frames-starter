{
  "name": "@lyaviache/next-fullstack-starter",
  "version": "0.1.0",
  "private": false,
  "author": "ilya.viache@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilyaviache/next-fullstack-starter"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "concurrently --kill-others \"next dev\" \"frames\"",
    "dev:next": "next dev",
    "dev:frames": "frames",
    "dev:prod": "concurrently --kill-others \"next dev -p 3000 \" \"frames\"",
    "docker:dev": "docker-compose -f docker-compose.dev.yaml up -d",
    "docker:prod": "docker-compose -f docker-compose.yaml up --build -d",
    "lint": "tsc --noEmit && eslint src --ignore-pattern 'src/public' --max-warnings 0",
    "lint:fix": "tsc --noEmit && eslint src --ignore-pattern 'src/public' --fix",
    "update-debugger": "npx degit github:framesjs/frames.js/examples/framesjs-starter/app/debug#main app/debug --force",
    "update-framesjs": "yarn upgrade frames.js@latest && yarn run update-debugger"
  },
  "dependencies": {
    "@farcaster/core": "^0.14.20",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.45.0",
    "@tanstack/react-query": "^5.51.18",
    "@tanstack/react-query-devtools": "^5.51.18",
    "axios": "^1.7.3",
    "class-variance-authority": "^0.7.0",
    "formik": "^2.4.6",
    "frames.js": "^0.18.1",
    "geist": "^1.3.1",
    "lucide-react": "^0.424.0",
    "next": "14.1.0",
    "next-themes": "^0.3.0",
    "nextjs-toploader": "^1.6.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-lazy-load-image-component": "^1.6.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@frames.js/debugger": "^0.3.7",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.18",
    "@types/react-lazy-load-image-component": "^1.5.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.31",
    "postcss-import": "^16.0.1",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
